class Solution(object):
    def minOperations(self, nums):
        n = len(nums)
        num1 = nums.count(1)
        if num1>0:
            return n - num1
        
        g = 0
        for i in nums:
            g = self.gcd(g,i)
        if g > 1:
            return -1
        
        min_len = n
        for i in range(n):
            g = 0
            for j in range(i,n):
                g = self.gcd(g,nums[j])
                if g == 1:
                    min_len = min(min_len , j-i+1)
                    break
        return min_len + n-2
    
    def gcd(self,n,m):
        if (m==0):
            return n
        return self.gcd(m,n%m)
