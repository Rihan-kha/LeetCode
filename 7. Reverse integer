class Solution(object):
    def reverse(self, x):
        """
        :type x: int
        :rtype: int
        """
        if x<0:
            rn = -1*int(str(x*-1)[::-1])
        else :
            rn = int(str(x)[::-1])
        if ((-2**31)>rn or rn>(2**31 -1)):
            return 0
        return rn