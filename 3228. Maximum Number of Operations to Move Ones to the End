class Solution(object):
    def maxOperations(self, s):
        count = 0
        c_1 = 0
        zero = False
        for i in s:
            if (i == '1'):
                if zero:
                    count += c_1
                    zero = False
                c_1 += 1
            elif (i == '0'):
                zero = True
        if zero:
            count += c_1
        return count
