class Solution(object):
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        count = 0
        st = ''
        for i in range(len(s)):
            if s[i] in st:
                if count<len(st):
                    count = len(st)
                st = st[st.index(s[i])+1::] + s[i]

            else:
                st = st+s[i]
        if (len(st)>count):
            count = len(st)
        return count
        
