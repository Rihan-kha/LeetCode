class Solution(object):
    def mySqrt(self, x):
        """
        :type x: int
        :rtype: int
        """
        if (x<=1):
            return  x
        low = 1
        high = x//2
        while(low<=high):
            mid = (low+high)//2
            if (mid*mid > x):
                high = mid-1
            elif(mid*mid < x):
                low = mid+1
                if (low*low >x):
                    return low-1
            else:
                return mid
