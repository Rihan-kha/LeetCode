class Solution(object):
    def longestCommonPrefix(self, strs):
        """
        :type strs: List[str]
        :rtype: str
        """
        r_str =strs[0]

        for s in strs:
            i=0
            new_str =''
            while (i<len(r_str) and i<len(s)):
                if (r_str[i] != s[i]):
                    break
                print(new_str)
                new_str = new_str + s[i]
                i+=1
            r_str = new_str

        return r_str
        
