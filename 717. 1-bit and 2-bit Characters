class Solution(object):
    def isOneBitCharacter(self, bits):
        """
        :type bits: List[int]
        :rtype: bool
        """
        n = len(bits)
        i=0
        while(i<n-1):
            if (bits[i]==1):
                i+=1
            i+=1
        if i==n:
            return False
        return True   
