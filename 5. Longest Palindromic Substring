class Solution(object):
    def longestPalindrome(self, s):
        """
        :type s: str
        :rtype: str
        """
        s1 = ''
        for i in range(len(s)):
            for j in range(len(s) , i,-1):
                st = s[i:j]
                if st == st[::-1]:
                    if (len(st)>len(s1)):
                        s1 = st
        return s1
