class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        parenthesis = {'o':'([{','c':')]}'}
        stack = []
        for p in s:
            if (p in parenthesis['o']):
                stack.append(p)
            elif (len(stack)>0):
                i = parenthesis['c'].index(p)
                if (stack[-1] != parenthesis['o'][i]):
                    return False
                stack.pop()
            else:
                return False
        if len(stack):
            print("Yes2")
            return False
        return True
        
