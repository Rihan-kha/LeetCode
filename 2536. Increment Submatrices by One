class Solution(object):
    def rangeAddQueries(self, n, queries):
        """
        :type n: int
        :type queries: List[List[int]]
        :rtype: List[List[int]]
        """
        r_lst = [[0]*n for j in range(n)]

        diff = [[0]*(n+1) for _ in range(n+1)]

        for r1 , c1, r2 , c2 in queries:
            diff[r1][c1] += 1
            diff[r2+1][c1] -= 1
            diff[r2+1][c2+1] +=1
            diff[r1][c2+1] -= 1

        for i in range(n):
            for j in range(n):
                x1 = 0 if i == 0 else r_lst[i - 1][j]
                x2 = 0 if j == 0 else r_lst[i][j - 1]
                x3 = 0 if i == 0 or j == 0 else r_lst[i - 1][j - 1]
                r_lst[i][j] = diff[i][j] + x1 + x2 - x3

        return r_lst
