class Solution(object):
    def pivotIndex(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        prefix = [0]*(len(nums)+1)
        for i in range(len(nums)):
            prefix[i+1] = nums[i] + prefix[i]

        for i in range(1,len(prefix)):
            if (prefix[i-1]-prefix[0] == prefix[-1]-prefix[i]):
                return i-1
        return -1
